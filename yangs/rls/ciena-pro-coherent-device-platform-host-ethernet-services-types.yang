    leaf tx-conditioning-type {
      type host-service-ethernet-fault-inject-type-enum;
      description
        "";
    }
    leaf auto-condition-client-on-link-degrade {
      type boolean;
      default "true";
      description
        "";
    }
    leaf rmon-enable {
      type boolean;
      description
        "";
    }
    leaf eth-service-odu-conditioning-type {
      type host-service-ethernet-client-network-odu-conditioning-enum;
      description
        "";
    }
    leaf eth-400GE-interop-mode {
      type boolean;
      default "false";
      description
        "Internal host control to determine whether 400GE client mapping should be compatible
         with ASIC AA version mapping (custom_400GE). This is only applicable to -O transmission-modes.";
    }
    description
      "";
